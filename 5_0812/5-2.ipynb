    "#利用if進行血壓值判斷\n",
    "#血壓應於80~120之間，符合為正常\n",
    "#否則，不正常#血壓應於80~120之間，符合為正常\n",
    "#備註:不能使用到邏輯運算子\n",
    "\n",
    "presure=int(input(\"請輸入血壓值:\"))\n",
    "if 80 <= presure <= 120:\n",
    "    print(\"血壓正常\")\n",
    "else:\n",
    "    print(\"血壓不正常\")"
   ]
